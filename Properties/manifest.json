{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
  "version": "{version}",
  "manifestVersion": "1.9",
  "id": "{appid}",
  "packageName": "com.company.example",
  "name": {
    "short": "TeamsCallBot",
    "full": "TeamsCallBot"
  },
  "developer": {
    "name": "TeamsCallBot",
    "mpnId": "",
    "websiteUrl": "{baseUrl}",
    "privacyUrl": "{baseUrl}/privacy",
    "termsOfUseUrl": "{baseUrl}/termsofuse"
  },
  "description": {
    "short": "Short description for TeamsCallBot.",
    "full": "Full description of TeamsCallBot."
  },
  "icons": {
    "outline": "../outline.png",
    "color": "../color.png"
  },
  "accentColor": "#FFFFFF",
  "configurableTabs": [
    {
      "configurationUrl": "{baseUrl}/config",
      "canUpdateConfiguration": true,
      "scopes": [
        "team",
        "groupchat"
      ]
    }
  ],
  "staticTabs": [
    {
      "entityId": "index",
      "name": "Personal Tab",
      "contentUrl": "{baseUrl}/tab",
      "websiteUrl": "{baseUrl}/tab",
      "scopes": [
        "personal"
      ]
    }
  ],
  "validDomains": [
    "{frontEndDomain}"
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "webApplicationInfo": {
    "id": "{appClientId}",
    "resource": "api://{frontEndDomain}/{appClientId}"
  }
}